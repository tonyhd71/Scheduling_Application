<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="298.0" prefWidth="317.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.ReportsMenuController">
   <children>
      <VBox layoutY="3.0" prefHeight="298.0" prefWidth="317.0">
         <children>
            <Label alignment="CENTER" prefHeight="17.0" prefWidth="590.0" text="Reports Menu" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Button fx:id="apptByMonthButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#clickApptByMonth" prefHeight="37.0" prefWidth="241.0" text="Appointments this Month" textAlignment="CENTER">
               <VBox.margin>
                  <Insets bottom="15.0" left="15.0" top="15.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
            <Button fx:id="schedForEachButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#schedForEachClick" prefHeight="37.0" prefWidth="240.0" text="Schedule for Each Consultant" textAlignment="CENTER">
               <VBox.margin>
                  <Insets bottom="15.0" left="15.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
            <Button fx:id="apptsThisWkBtn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#apptsThisWkBtnClick" prefHeight="37.0" prefWidth="240.0" text="Appointments this Week" textAlignment="CENTER">
