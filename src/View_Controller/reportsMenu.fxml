                  <Insets bottom="15.0" left="15.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
            <Button fx:id="customersByLoc" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#customersByLocClick" prefHeight="37.0" prefWidth="240.0" text="Appointments by Location" textAlignment="CENTER">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <VBox.margin>
                  <Insets left="15.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="returnBackButton" mnemonicParsing="false" onAction="#returnToMenuOptions" text="Back">
               <VBox.margin>
                  <Insets left="15.0" top="50.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
